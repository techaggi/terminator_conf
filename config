  go_down = <Primary><Alt>j
  go_left = <Primary><Alt>h
  go_right = <Primary><Alt>l
  split_horiz = <Primary><Alt>o
  split_vert = <Primary><Alt>i
  close_term = <Primary><Alt>0
  close_window = <Primary><Shift>o
  resize_up = <Primary><Alt>Up
  resize_down = <Primary><Alt>Down
